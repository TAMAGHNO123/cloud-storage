generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model File {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  originalName String
  mimeType     String
  fileName     String
  size         Int
  filePath     String
  createdAt    DateTime @default(now())
}
